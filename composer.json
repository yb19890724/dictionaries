{
    "name": "phpno1/dictionary",
    "description": "laravel data dictionary mapping",
    "license": "MIT",
    "authors": [
        {
            "name": "hyb",
            "email": "354037968@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "phpunit/phpunit": "~6.0",
        "laravel/framework": "5.5.*"
    },
    "autoload":{
        "psr-4":{
            "App\\": "app/",
            "Phpno1\\Dictionaries\\":"src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Phpno1\\Dictionaries\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phpno1\\Architecture\\Providers\\DictionaryProvider"
            ]
        }
    }
}
